ToEdit={colorToEdit}\r\n              setColorToEdit={setColorToEdit}\r\n              setData={setData}\r\n            />\r\n          </div>\r\n        </div>\r\n        <div className=\"pt-5 \">\r\n          <Reusable_table\r\n            nameOfColumn={header_colorTable}\r\n            dataOfRows={data}\r\n            placeholder_searchBox=\"قسمتی از عنوان را وارد کنید\"\r\n            additionField={additionField}\r\n            having_searchBox={true}\r\n            operation={true}\r\n            show_compo={true}\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n","C:\\Users\\f.alizadeh\\Desktop\\alizadeh\\admin_panel\\src\\components\\pages\\5-colour\\Operations_colour.jsx",[],[],"C:\\Users\\f.alizadeh\\Desktop\\alizadeh\\admin_panel\\src\\components\\pages\\5-colour\\add_or_edit_color\\AddNewColor.jsx",[],[],"C:\\Users\\f.alizadeh\\Desktop\\alizadeh\\admin_panel\\src\\components\\pages\\5-colour\\FormikHelper_colour.jsx",["598","599","600"],[],"import * as Yup from \"yup\";\r\nimport {\r\n  createNewColorService,\r\n  updateColorService,\r\n} from \"../../../services/CRUD_categoryService\";\r\nimport swal from \"sweetalert\";\r\n\r\n//====================== 📍initialValues =====================\r\nexport const initialValues = {\r\n  title: \"white\",\r\n  code: \"#ffffff\",\r\n};\r\n\r\n//====================== 📍onSubmit ===========================\r\nexport const onSubmit = async (\r\n  values,\r\n  actions,\r\n  colorToEdit,\r\n  setData,\r\n  setColorToEdit,\r\n  setColorPickerValue\r\n) => {\r\n\r\n  //===== ⭐ ویرایش یکی از رنگ ها =====================\r\n  if (colorToEdit) {\r\n    const res = await updateColorService(values, colorToEdit.id);\r\n    if (res.status == 200) {\r\n      swal(\"ویرایش شد!...\", res.data.message, \"success\");\r\n      setColorToEdit(res.data.data);\r\n      setData((oldData) => {\r\n        let newData = [...oldData];\r\n        let index = newData.findIndex((d) => d.id == colorToEdit.id);\r\n        newData[index] = res.data.data;\r\n        return newData;\r\n      });\r\n    }\r\n    // ======= ⭐ افزودن یک رنگ جدید ===================\r\n  } else {\r\n    const res = await createNewColorService(values);\r\n    if (res.status == 201) {\r\n      swal(\"ثبت شد!...\", res.data.message, \"success\");\r\n      setData((oldData) => [...oldData, res.data.data]);\r\n      setColorPickerValue(\"#000\");\r\n      actions.resetForm();\r\n    }\r\n  }\r\n};\r\n\r\n//====================== 📍validationSchema ===================\r\nexport const validationSchema = Yup.object({});\r\n","C:\\Users\\f.alizadeh\\Desktop\\alizadeh\\admin_panel\\src\\components\\pages\\1-productGroup\\ProductGroupManagement.jsx",["601","602","603","604"],[],"import React, { useEffect } from \"react\";\nimport { UpperPartPages } from \"../../general_compo/reusable_operations/UpperPartPages\";\nimport { getCategoriesService } from \"../../../services/CRUD_categoryService\";\nimport { useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport { Operations_product } from \"./table_additon/Operations_product\";\nimport moment from \"jalali-moment\";\nimport { Reusable_table } from \"../../general_compo/reusable_table/reusable_table1/Reusable_table\";\n\nexport const ProductGroupManagement = () => {\n  const params = useParams();\n  const [data, setData] = useState([]);\n  // const [forceRender, setForceRender] = useState(0); //برای اینکه هرجا خواستیم جدول رو رندر مجدد کنیم\n\n  //========= هدرهای جدول📍==============\n  const headers_productTable = [\n    { field: \"id\", title: \"id\" },\n    { field: \"title\", title: \"عنوان محصول\" },\n    { field: \"parent_id\", title: \"والد\" },\n  ];\n  //======================================\n  const handleGetCategories = async () => {\n    const res = await getCategoriesService(params.productId);\n    try {\n      if (res.status == 200) {\n        setData(res.data.data);\n      }\n    } catch {}\n  };\n  //======================